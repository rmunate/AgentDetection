    "homepage": "https://github.com/rmunate/AgentDetection",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Rmunate\\AgentDetection\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Raul Mauricio UÃ±ate Castro",
            "email": "raulmauriciounate@gmail.com",
            "homepage": "https://github.com/rmunate",
            "role": "owner"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0",
        "jaybizzle/crawler-detect": "^1.2",
        "mobiledetect/mobiledetectlib": "^4.8"
